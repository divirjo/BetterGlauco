{
    "version": 2,
    "name": "vercel_build_staticfiles",
    "builds": [
        {
            "src": "BetterGlauco/wsgi.py",
            "use": "@vercel/python"
        },
        {
          "src": "vercel_build.sh",
          "use": "@vercel/static-build",
          "config": {
            "distDir": "staticfiles_build"
            }
          }
    ],
    "routes": [
        {
          "src": "/static/(.*)",
          "dest": "/static/$1"
        },
        {
          "src": "/(.*)",
          "dest": "BetterGlauco/wsgi.py"
        }
      ]
}
